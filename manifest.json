{
    "manifest_version": 2,

    "name": "TabSaver",
    "description": "Allow to save and load every tabs.",
    "version": "1.0",

    "browser_action": {
        "default_icon": "img/icon.png",
        "default_popup": "tabsaver.html",
        "default_title": "TabSaver"
    },

    "permissions": [
        "activeTab",
        "storage",
        "tabs",
        "https://ajax.googleapis.com/"
    ]
}
